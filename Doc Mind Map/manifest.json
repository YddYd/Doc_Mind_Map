{
  "manifest_version": 3,
  "name": "StructDoc â€” Google Docs Outliner",
  "version": "0.6.0",
  "description": "Re-outline any Google Doc with Chrome Built-in AI Writer and visualize as a focusable tree; Summarize Article; Cooperative workspace; optional export to a new page.",
  "permissions": [
    "tabs",
    "sidePanel",
    "identity",
    "storage"
  ],
  "host_permissions": [
    "https://docs.google.com/*",
    "https://docs.googleapis.com/*",
    "https://www.googleapis.com/*"
  ],
  "background": {
    "service_worker": "service_worker.js"
  },
  "action": {
    "default_title": "StructDoc"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "trial_tokens": [
    "YOUR_WRITER_API_ORIGIN_TRIAL_TOKEN"
  ],
  "oauth2": {
    "client_id": "YOUR_CLIENT_ID.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/documents",
      "https://www.googleapis.com/auth/drive.readonly"
    ]
  }
}